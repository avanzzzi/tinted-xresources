! base24 Builtin Light
! Scheme author: FredHappyface (https://github.com/fredHappyface)
! Template author: Tinted Theming (https://github.com/tinted-theming)

#define base00 #ffffff
#define base01 #000000
#define base02 #555555
#define base04 #888888
#define base05 #a1a1a1
#define base06 #bbbbbb
#define base07 #ffffff
#define base08 #bb0000
#define base09 #bbbb00
#define base0A #5555ff
#define base0B #00bb00
#define base0C #00bbbb
#define base0D #0000bb
#define base0E #bb00bb
#define base0F #5d0000
#define base12 #ff5555
#define base13 #ffff55
#define base14 #55ff55
#define base15 #55ffff
#define base16 #5555ff
#define base17 #ff55ff

*foreground:   base05
#ifdef background_opacity
*background:   [background_opacity]base00
#else
*background:   base00
#endif
*cursorColor:  base05

*color0:       base00
*color1:       base08
*color2:       base0B
*color3:       base0A
*color4:       base0D
*color5:       base0E
*color6:       base0C
*color7:       base06

*color8:       base02
*color9:       base12
*color10:      base14
*color11:      base13
*color12:      base16
*color13:      base17
*color14:      base15
*color15:      base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
*color16:      base09
*color17:      base0F
*color18:      base01
*color19:      base02
*color20:      base04
*color21:      base06
